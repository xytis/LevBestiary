- var mUser = inpUser
- var content = iCont
- var entries= iEntries

html
  head
    link(rel="stylesheet", type="text/css", href="/styles.css")      
body
  div#wrapper
    div#header
      include ./Wrap/header
    div#container
      div#navigation
        include ./Wrap/hNav
      div#leftcolumn
        include ./Wrap/navigationBar
      div#rightcolumn
        case content
          when "info"
            include ./includes/accountInfoContent
          when "create"
            include ./includes/createAccount
          when "newEntry"
            include ./includes/newEntry
    div#footer
      include ./Wrap/footer
    

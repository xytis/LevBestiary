- var mUser = inpUser
- var content = iCont
- var entries= iEntries

html
  head
    link(rel="stylesheet", type="text/css", href="/styles.css")      
body
  div#wrapper
    div#header
      div.headerTitle Lev Bestiary
      div.subHeaderTitte 
        i Bestiary of the Void
    div#container
      div#navigation
        include ./Wrap/hNav.jade
      div#leftcolumn
        include ./Wrap/navigationBar.jade
      div#rightcolumn
        case content
          when "info"
            include ./includes/accountInfoContent.jade
          when "create"
            include ./includes/createAccount.jade
          when "newEntry"
            include ./includes/newEntry.jade
    div#footer
      p
        i Made by Kasparas as a training task
    
